	email = swiftwind0405@gmail.com
	name = Stanley
[push]
	default = current
	autoSetupRemote = true
[pull]
	rebase = true
	autoSetupRemote = true
[core]
	autocrlf = input
	ignorecase = false
[http]
	sslverify = false
[pager]
	branch = false
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	process = git-lfs filter-process
	smudge = git-lfs smudge -- %f
